{
  "name": "@patter/patter-core-styles",
  "version": "1.3.5",
  "description": "Core Patter SCSS UI Kit",
  "main": "scss/main.scss",
  "scripts": {
    "publish-package": "npm publish --access public",
    "build-main": "sass scss/main.scss dist/main.css",
    "build-wordpress": "sass scss/wordpress-public.scss dist/wordpress-public.css",
    "build-wordpress-admin": "sass scss/wordpress-admin.scss dist/wordpress-admin.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dreem-Media/patter-core-styles.git"
  },
  "author": "Alex Whitlock <alex@patter.co.uk> (https://patter.co.uk/)",
  "contributors": [
    "Alex Whitlock <zanderwhitlock@gmail.com>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dreem-Media/patter-core-styles/issues"
  },
  "homepage": "https://github.com/Dreem-Media/patter-core-styles#readme",
  "keywords": [
    "scss"
  ],
  "exports": {
    "./js-utils": "./js/public/_utils.js",
    "./js/public": "./js/public/_index.js",
    "./js/admin": "./js/admin/_index.js",
    "./js/block-editor": "./js/block-editor/_index.js",
    "./scss/breakpoints": "./scss/abstracts/_breakpoints.scss",
    "./scss/main": "./scss/main.scss",
    "./scss/wordpress-public": "./scss/wordpress-public.scss",
    "./scss/wordpress-admin": "./scss/wordpress-admin.scss",
    "./scss/wordpress-block-editor": "./scss/wordpress-block-editor.scss",
    "./scss/aos-animations": "./scss/aos-animations.scss",
    "./scss/independent-styles-setup": "./scss/independent-styles-setup.scss"
  },
  "dependencies": {
    "aos": "^2.3.4"
  }
